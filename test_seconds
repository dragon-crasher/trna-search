#!/bin/bash

# Start timing
SECONDS=0

# Simulate some processing (replace with your actual commands)
sleep 10  # Simulate a task that takes time

# Calculate duration in minutes and seconds
duration=$(( SECONDS / 60 ))
remaining_seconds=$(( SECONDS % 60 ))

echo "Duration: $duration minutes and $remaining_seconds seconds"
